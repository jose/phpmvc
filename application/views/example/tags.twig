  MutableDateTime mutableDateTime1 = new MutableDateTime(mutableDateTime0.ROUND_NONE, mutableDateTime0.ROUND_HALF_CEILING, mutableDateTime0.ROUND_FLOOR, mutableDateTime0.ROUND_HALF_FLOOR, mutableDateTime0.ROUND_HALF_FLOOR, mutableDateTime0.ROUND_HALF_EVEN, mutableDateTime0.ROUND_NONE);
  EvoSuiteFile evoSuiteFile0 = null;
  byte[] byteArray0 = new byte[0];
  FileSystemHandling.shouldAllThrowIOExceptions();
