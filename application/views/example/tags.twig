{% extends "_templates/base.twig" %}
{% block content %}

<!--<h1>Tags</h1>-->

<!-- TestCase and a box to put tags -->
<div class="row row-offcanvas row-offcanvas-left">
<form id="_form" role="form" method="post" action="{{ constant('URL') }}example/action">

  <!-- TestCase -->
  <div class="col-xs-12 col-sm-9 col-md-8" style="padding-left: 0;">
    <div class="row center-block">
      <!-- Tab header -->
      <ul class="nav nav-tabs" id="test_case_tab">
        <li class="active "><a href="#test" aria-controls="test" role="tab" data-toggle="tab">{{ constant('TEST_CASE') }}</a></li>
      </ul>
      <!-- Tab content -->
      <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="test">
          <pre class="scrollable" style="height: 670px;"><code class="language-java">
