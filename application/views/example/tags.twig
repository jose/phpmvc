  mutableDateTime1.getMillisOfSecond();
  mutableDateTime0.addHours(mutableDateTime1.ROUND_CEILING);
  int int1 = 2775;
  mutableDateTime1.getCenturyOfEra();
  String string0 = "{fmN^IHDDF\\;jb?Z@";
  // Undeclared exception!
  try {
    LocalDate.parse(string0);
    fail("Expecting exception: IllegalArgumentException");
  
  } catch(IllegalArgumentException e) {
